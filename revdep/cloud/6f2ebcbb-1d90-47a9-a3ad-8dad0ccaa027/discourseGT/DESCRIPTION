Package: discourseGT
Type: Package
Title: Analyze Group Patterns using Graph Theory in Educational
        Settings
Version: 1.1.7
Date: 2021-11-3
Authors@R: c(
  person("Albert","Chai",   role=c("aut"), email = "albertchai@ucsd.edu"),
